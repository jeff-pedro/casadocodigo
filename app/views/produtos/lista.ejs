<!DOCTYPE html>
<html>
  <head>
    <meta>
    <title></title>
  </head>
  <body>
    <table>
      <thead>
        <th>ID</th>
        <th>TÍTULO</th>
        <th>DESCRIÇÃO</th>
        <th>PREÇO</th>
      </thead>
      <tbody>
            <% for(var i = 0; i < lista.length; i++) { %>
            <tr>
                <td><%= lista[i].id %></td>
                <td><%= lista[i].titulo %></td>
                <td><%= lista[i].descricao %></td>
                <td><%= lista[i].preco %></td>
            </tr>
            <% } %>
      </tbody>
    </table>
  </body>
</html>
